(defun extract-text-in-tag (url start-tag end-tag)
  (interactive "MURL: ")
  (switch-to-buffer "*temp-bookmark*")
  (insert-buffer (url-retrieve-synchronously url)
  (search-forward start-tag nil t)
  (setq title-start (point))
  (search-forward end-tag nil t)
  (backward-char 2)
  (setq title-end (point))
  (let ((text (buffer-substring title-start title-end)))
    text))

(defun extract-text-in-tag (url start-tag end-tag)
  (interactive "MURL: ")
  (switch-to-buffer "*temp-bookmark*")
  (insert-buffer (url-retrieve-synchronously url))
  (re-search-forward "<title>\\(.*\\)</title>")
  (backward-char 8)
  (setq title-end (point))
  (let ((text (buffer-substring title-start title-end)))
    text))
)
<title>asds</title>
(setq extracted-title (extract-text-in-tag "https://www.2dogsdesign.com/what-is-meta-description/" "<title>" "</"))
(setq extracted-title (extract-text-in-tag "https://www.wikipedia.org" "meta name=\"description\" content=\"" "\">"))
(insert extracted-title)
 
